{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:api": "cross-env REACT_APP_API_URL=https://console-test-cn.tensorsecurity.cn vite",
    "dev:micro-front-iframe": "cross-env REACT_APP_ENV=micro-front-iframe vite",
    "dev:zgyd": "cross-env REACT_APP_SUBJECT=zgyd craco start",
    "build": "vite build",
    "build:local": "node --max_old_space_size=4096 ./node_modules/vite/bin/vite.js build",
    "prepare": "husky install",
    "lint": "npm run lint:style && npm run lint:js",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src && npm run lint:style",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint --fix \"src/**/*.scss\"",
    "prettier": "prettier -c --write **/*/*.{ts,tsx,js,scss,json}"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.scss": "npm run lint:style",
    "**/*.{js,jsx,tsx,ts,scss,md,json}": [
      "prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js"
  },
